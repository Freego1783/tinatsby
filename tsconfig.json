{
    "compilerOptions": {
        "target": "ES2019",
        "module": "commonjs",
        "lib": ["dom", "es2019", "es2020"],
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true
    },
    "include": ["gatsby-*.ts", "plugins/**/*/gatsby-*.ts", "src/**/*", "tina/**/*"]
}
